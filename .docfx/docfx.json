{
  "metadata": [
    {
      "src": [
        {
          "src": "../UnityCsReference/Projects/CSharp/",
          "files": [
            "**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "filter": "filterConfig.yml",
      "disableGitFeatures": true
    }
  ],
  "build":
  {
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ]
      },
      {
        "src": "../UnityCsReference/",
        "files":[
          "/*.md"
        ]
      }
    ]
  },
  "globalMetadata": {
    "_enableSearch": true,
    "pdf": false
  }
}